{
  "version": "1.0.0",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "start": "tsup --watch",
    "build": "tsup",
    "test": "vitest run",
    "prepare": "tsup",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "prothesmies",
  "author": "Ioannis Mertzanis",
  "repository": {
    "type": "git",
    "url": "git://github.com/imertz/prothesmies.git"
  },
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "20 KB"
    },
    {
      "path": "dist/index.mjs",
      "limit": "20 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^11.0.0",
    "size-limit": "^11.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^3.0.0"
  },
  "dependencies": {}
}
